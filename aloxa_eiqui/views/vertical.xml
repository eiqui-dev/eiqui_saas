<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
	
		<!-- FORM -->
		<record id="eiqui_vertical_form" model="ir.ui.view">
			<field name="name">Verticales</field>
			<field name="model">eiqui.vertical</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="Verticales">
					<sheet>
						<group>
							<field name="name"/>
							<field name="image"/>
							<field name="description"/>
							<field name="modules"/>
						</group>
					</sheet>
				</form>
			</field>
		</record>
		
		<!-- TREE -->
		<record id="eiqui_vertical_tree" model="ir.ui.view">
			<field name="name">Verticales</field>
			<field name="model">eiqui.vertical</field>
			<field name="type">tree</field>
			<field name="arch" type="xml">
				<tree string="Verticales">
					<field name="name"/>
					<field name="description"/>
				</tree>
			</field>
		</record>

	</data>
</openerp>